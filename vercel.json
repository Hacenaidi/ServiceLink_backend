{
  "builds": [
    {
      "src": "serviceLink/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.12.6" }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "serviceLink/wsgi.py"
    }
  ]
}